---
const {title, deployUrl, gitUrl, images} = Astro.props
import Slider from "../Slider";
---

<div class='border px-5 py-5 bg-white rounded-md w-1/3'>
    <Slider images={images} title={title} client:load/>
    <h2 class="simple-text text-center font-semibold text-lg">{title.toUpperCase()}</h2>
    <slot/>
</div>